{
  "name": "exampleProgram",
  "version": "1.0.0",
  "description": "an example program.",
  "author": "genshen <genshenchu@gmail.com>",
  "commands": {
    "debug": "cmake ../",
    "release": "cmake ../",
    "test": "cmake ../"
  },
  "compilers": {
    "cc": "clang",
    "cxx": "clang++"
  },
  "packages": {
    "git": {
      "googletest": {
        "path": "https://github.com/google/googletest.git",
        "tag": "release-1.8.0"
      },
      "kiwi": {
        "path": "https://git.gensh.me/genshen/kiwi.git",
        "hash": "21513034630d1e3fd4313e6571795da685c775e5"
      }
    },
    "files": {
      "agrs": {
        "path": "https://raw.githubusercontent.com/Taywee/args/master",
        "files": {
          "args.hxx": "args.hpp"
        },
        "build": [
          "CP args.hpp {INCLUDE}"
        ]
      },
      "nlohmann": {
        "path": "https://github.com/nlohmann/json/raw/v3.1.2/single_include/nlohmann",
        "files": {
          "json.hpp": "json.hpp"
        },
        "build": [
          "CP json.hpp {INCLUDE}"
        ]
      },
      "concurrentqueue": {
        "path": "https://raw.githubusercontent.com/cameron314/concurrentqueue/master",
        "files": {
          "concurrentqueue.h": "concurrentqueue.hpp"
        },
        "build": [
          "CP concurrentqueue.hpp {INCLUDE}"
        ]
      }
    }
  }
}
